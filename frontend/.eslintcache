ktop\\UNIPR2020\\Progetti\\Project_github\\frontend\\src\\reducers\\UserReducer.js",[],"C:\\Users\\kemga\\Desktop\\UNIPR2020\\Progetti\\Project_github\\frontend\\src\\reducers\\CategoriesReducer.js",[],"C:\\Users\\kemga\\Desktop\\UNIPR2020\\Progetti\\Project_github\\frontend\\src\\reducers\\CartReducer.js",["126","127","128","129"],"/***********************************************************************\r\n************ Authors:    Christian KEMGANG NGUESSOP ********************\r\n************                        &                 ******************\r\n************             Linda Rodiere TCHOUFFONG METOU ****************\r\n************ Version:    1.0.0 *****************************************\r\n***********************************************************************/\r\nimport React, { useState } from \"react\";\r\nimport { CART_PRODUCT, GET_NUMBERS_BASKET } from \"../actions/CartsActions\";\r\nimport { useSelector } from \"react-redux\";\r\n\r\n//const proData = useSelector((state) => state.productsReducer);\r\n\r\n//console.log(\"ALL PRO >\", proData)\r\n\r\nconst initialState = {\r\n    products: {}, cart:[], basketNumbers : 0 , totalPrice: 0};\r\n\r\n\r\nexport default function cartsReducer(state = initialState, action) {\r\n\r\n    switch (action.type) {\r\n        case CART_PRODUCT:\r\n\r\n            let total = 0;\r\n\r\n            action.payload.map((p) => {\r\n                total += p.price\r\n            });\r\n\r\n            return {\r\n                cart: [...state.cart, action.payload],\r\n                basketNumbers: state.basketNumbers + 1,\r\n                totalPrice: state.totalPrice += total\r\n            }\r\n\r\n        case GET_NUMBERS_BASKET: \r\n            return {\r\n                ...state\r\n            }\r\n\r\n        default:\r\n            return state;\r\n    }\r\n}\r\n","C:\\Users\\kemga\\Desktop\\UNIPR2020\\Progetti\\Project_github\\frontend\\src\\reducers\\ProductsReducer.js",[],"C:\\Users\\kemga\\Desktop\\UN